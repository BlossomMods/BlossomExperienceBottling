  "authors": [
    "CodedSakura"
  ],
  "contact": {
    "website": "https://blossom.codedsakura.dev",
    "repo": "https://github.com/BlossomMods/BlossomExperienceBottling"
  },
  "license": "MIT",
  "icon": "assets/blossom-experience-bottling/icon.png",
  "environment": "*",
  "entrypoints": {
